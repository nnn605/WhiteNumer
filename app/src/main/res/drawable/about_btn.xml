<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--设置按钮默认地显示地图片-->
    <item android:state_pressed="false" android:state_focused="false"
        android:drawable="@drawable/btn_about1"/>
    <!--设置按钮显示地图片-->
    <item android:state_pressed="true" android:drawable="@drawable/btn_about2"/>
    <!--设置按钮长按时显示地图片-->
    <item android:state_focused="true" android:drawable="@drawable/btn_about2"/>

</selector>